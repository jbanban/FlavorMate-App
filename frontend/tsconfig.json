{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [ "*" ] ,
      "@constants/*": ["./constants/*"],
      "@components/*": ["./components/*"],
      "@api/*": ["./api/*"],
      "@hooks/*": ["hooks/*"],
    },
      "jsx": "react-native",
      "strict": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
}
